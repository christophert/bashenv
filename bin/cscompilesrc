compile() {
    if [ ! -z $1 ] && [ -e $1 ]; then
        gcc -std=c99 -Wall -Wextra -pedantic $1.c -o $1.o
        if [ $? -eq 0 ]; then
            echo "Compilation finished"
        else
            echo "Compliation error"
        fi
    else
        echo "No input files or does not exist"
    fi
}
